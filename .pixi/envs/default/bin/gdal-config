#!/bin/sh
CONFIG_LIBS="-L/home/anna/people_places_germany/.pixi/envs/default/lib -lgdal"
CONFIG_DEP_LIBS="-ldl -larchive -liconv -lssl -lcrypto -llzma -ldeflate -llz4 -lgeotiff -ltiff -lLerc -lzstd -ljpeg -lpng -lgif -lblosc -lwebp -lkmlbase -lkmldom -lkmlengine -lminizip -luriparser -lz -lpcre2-8 -lspatialite -lsqlite3 -lxerces-c -lexpat -lxml2 -lgeos_c -lcurl -lproj -ljson-c -lpthread -lstdc++ -lm"
CONFIG_PREFIX="/home/anna/people_places_germany/.pixi/envs/default"
CONFIG_CFLAGS="-I/home/anna/people_places_germany/.pixi/envs/default/include"
CONFIG_DATA="/home/anna/people_places_germany/.pixi/envs/default/share/gdal"
CONFIG_VERSION="3.10.0"
CONFIG_GNM_ENABLED=yes
CONFIG_FORMATS="JPEG raw GTIFF MEM vrt Derived GTI HFA SDTS NITF GXF AAIGrid CEOS SAR_CEOS XPM DTED JDEM Envisat ELAS FIT L1B RS2 ILWIS RMF Leveller SGI SRTMHGT IDRISI GSG ERS PALSARJaxa DIMAP GFF COSAR PDS ADRG COASP TSX Terragen BLX MSGN TIL R northwood SAGA XYZ AVIF HEIF ESRIC HF2 KMLSUPEROVERLAY CTG ZMap NGSGEOID IRIS MAP CALS SAFE SENTINEL2 PRF MRF WMTS GRIB BMP TGA STACTA SNAP_TIFF BSB AIGrid USGSDEM AirSAR OZI PCIDSK SIGDEM RIK STACIT PDF PNG GIF WCS HTTP netCDF Zarr DAAS EEDA FITS HDF5 PLMOSAIC WMS OGCAPI WEBP HDF4 Rasterlite MBTiles PostGISRaster KEA JP2OpenJPEG TileDB PCRaster MEM GeoJSON TAB Shape KML VRT AVC GML CSV DGN GMT NTF S57 Tiger Geoconcept GeoRSS DXF PGDump GPSBabel EDIGEO SXF OpenFileGDB WAsP Selafin JML VDV FlatGeobuf MapML MiraMon JSONFG SDTS GPX GMLAS SVG CSW LIBKML NAS PLSCENES WFS OAPIF NGW Elastic Idrisi PDS SQLite GeoPackage OSM VFK MVT PMTiles AmigoCloud Carto ILI PG XLSX XLS CAD Parquet Arrow GTFS ODS LVBAG"
CONFIG_PLUGINDIR="/home/anna/people_places_germany/.pixi/envs/default/lib/gdalplugins"
usage()

{
	cat <<EOF
Usage: gdal-config [OPTIONS]
Options:
	[--prefix[=DIR]]
	[--libs]
	[--dep-libs]
	[--cflags]
	[--datadir]
	[--version]
	[--ogr-enabled]
	[--gnm-enabled]
	[--formats]
	[--plugindir]
EOF
	exit $1
}

if test $# -eq 0; then
	usage 1 1>&2
fi

case $1 in
  --libs)
    echo $CONFIG_LIBS
    ;;

  --dep-libs)
    echo $CONFIG_DEP_LIBS
    ;;

  --cflags)
    echo $CONFIG_CFLAGS
    ;;

  --datadir)
    echo $CONFIG_DATA
    ;;

  --prefix)
    echo $CONFIG_PREFIX
    ;;

  --version)
    echo $CONFIG_VERSION
    ;;

  --ogr-enabled)
    echo yes
    ;;

  --gnm-enabled)
    echo $CONFIG_GNM_ENABLED
    ;;

  --formats)
    echo $CONFIG_FORMATS
    ;;

  --plugindir)
    echo $CONFIG_PLUGINDIR
    ;;

  *)
    usage 1 1>&2
    ;;

esac
